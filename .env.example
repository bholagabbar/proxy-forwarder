PORT=8000
VERBOSE=false

# Single upstream proxy base URL (credentials go here, not in the client)
UPSTREAM_PROXY_HOST=http://user:pass@proxy.example.com

# --- Strategy: static port ---
# PROXY_PORT=8000

# --- Strategy: port range (random at startup, or ET-minute per request) ---
# PROXY_PORT_LOWER=8001
# PROXY_PORT_UPPER=8050

# --- Strategy: fixed proxy list (JSON array) ---
# USE_PROXY_LIST=true
# PROXY_NUM=0
# PROXY_LIST=["http://user:pass@proxy1.example.com:10001","http://user:pass@proxy2.example.com:10002"]
